{
  "name": "delete-dwpcii",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "build": "npm run b:c && npm run b:s",
    "clean:logs": "rm -rf ./server/logs/*.log",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "cleans": "rm -rf ./dist",
    "dev": "NODE_ENV=development DEBUG=dwpcii:server babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "start": "node ./dist/bin/www"
  },
  "dependencies": {
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "materialize-css": "^1.0.0-rc.2",
    "method-override": "^3.0.0",
    "mongoose": "^7.6.5",
    "morgan": "~1.9.1",
    "webpack": "^5.76.1",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "winston": "^3.10.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "babel-watch": "^7.8.1",
    "core-js": "^3.32.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
