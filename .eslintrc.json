{
  "parser": "@babel/eslint-parser",
  "plugins": ["prettier"],
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "no-console":"off",
      "prettier/prettier": ["error"]
  },
  "globals": {
      "__rootdir": "readonly",
      "M": "readonly"
  }
}