{
  "parser": "@babel/eslint-parser",
  "extends":["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules":{
    "no-console":"off",
    "prettier/prettier": ["error"]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "settings": {
    "import/resolver" : { "babel-module" : {}}
  },
  "globals": {
    "__rootdir": "readonly",
    "M": "writable"
  }
}